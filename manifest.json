{
    "name": "PowerBI Report AutoRefresh",
    "version": "0.2",
    "description": "This extension will help you to reload your power bi reports",
    "permissions": ["tabs","background","activeTab", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icons/power_bi128.png"
    },
    "icons": {
        "128": "icons/power_bi128.png"
      },
      "web_accessible_resources": [
        "popup.js",
        "options.js",
        "utils.js",
        "createIntervalOptions.js"
     ],
    "manifest_version": 2
  }